                            <th rowspan="2" style="border: 2px solid black; background-color: blueviolet; color: white;">Rut</th>
                            <th rowspan="2" style="border: 2px solid black; background-color: blueviolet; color: white;">Nombre</th>
                            <th rowspan="2" style="border: 2px solid black; background-color: blueviolet; color: white;">Apellido</th>
                            <th rowspan="2" style="border: 2px solid black; background-color: blueviolet; color: white;">Edad</th>
                            <th rowspan="2" style="border: 2px solid black; background-color: blueviolet; color: white;">Talla</th>
                            <th rowspan="2" style="border: 2px solid black; background-color: blueviolet; color: white;">Peso</th>
                            <th colspan="2" style="border: 2px solid black; height: 2px; line-height: 2px; background-color: blueviolet; color: white;">M. Adiposa</th>
                            <th colspan="2" style="border: 2px solid black; height: 2px; line-height: 2px; background-color: blueviolet; color: white;">M. Muscular</th>
                            <th rowspan="2" style="border: 2px solid black; background-color: blueviolet; color: white;">6 Pliegues</th>
                            <th rowspan="2" style="border: 2px solid black; background-color: blueviolet; color: white;">I.M.O.</th>
                        </tr>
                        <tr style="text-align: center">
                            <th style="border: 2px solid black; background-color: blueviolet; color: white;">Porcentaje</th>
                            <th style="border: 2px solid black; background-color: blueviolet; color: white;">Kilogramo</th>
                            <th style="border: 2px solid black; background-color: blueviolet; color: white;">Porcentaje</th>
                            <th style="border: 2px solid black; background-color: blueviolet; color: white;">Kilogramo</th>
                        </tr>
                    </thead>
                    <tbody id="jugadoresUsersNutri" style="border: 3px solid black">
                        @foreach (var oFutbolista in Model.FutbolistaViewModelNutriList)
                        {
                            if (oFutbolista.CategoriaFutbolista == 1)
                            {
                                <tr style="text-align: center">
                                    <td style="border: 2px solid black"><a href="@Url.Content("~/Nutri/DatosJugador/?RutFutbolista=" + @oFutbolista.RutFutbolista)">Ver Detalles</a></td>
                                    <td style="border: 2px solid black">@oFutbolista.RutFutbolista</td>
                                    <td style="border: 2px solid black">@oFutbolista.NomFutbolista</td>
                                    <td style="border: 2px solid black">@oFutbolista.ApPaternoFutbolista</td>
                                    <td style="border: 2px solid black">@oFutbolista.EdadFutbolista</td>

                                    @if (Model.evaluacionNutricionalViewModels != null)
                                    {
                                        foreach (var oEvNutricional in Model.evaluacionNutricionalViewModels)
                                        {
                                            if (oEvNutricional.futbolista_id_futbolista == oFutbolista.id_futbolista)
                                            {
                                                <td style="border: 2px solid black">@oEvNutricional.talla_estatura cm.</td>
                                                <td style="border: 2px solid black">@oEvNutricional.peso Kg.</td>
                                                if (oEvNutricional.estado_uno == 1)
                                                {
                                                    <td style="border: 2px solid black; background-color: limegreen; color: white;">@oEvNutricional.porcent_masa_adiposa</td>
                                                }
                                                else if (oEvNutricional.estado_uno == 2)
                                                {
                                                    <td style="border: 2px solid black; background-color: yellow; color: black;">@oEvNutricional.porcent_masa_adiposa</td>
                                                }
                                                else if (oEvNutricional.estado_uno == 3)
                                                {
                                                    <td style="border: 2px solid black; background-color: red; color: white;">@oEvNutricional.porcent_masa_adiposa</td>
                                                }

                                                if (oEvNutricional.estado_uno_dos == 1)
                                                {
                                                    <td style="border: 2px solid black; background-color: limegreen; color: white; ">@oEvNutricional.kilo_masa_adiposa</td>
                                                }
                                                else if (oEvNutricional.estado_uno_dos == 2)
                                                {
                                                    <td style="border: 2px solid black; background-color: yellow; color: black;">@oEvNutricional.kilo_masa_adiposa</td>
                                                }
                                                else if (oEvNutricional.estado_uno_dos == 3)
                                                {
                                                    <td style="border: 2px solid black; background-color: red; color: white;">@oEvNutricional.kilo_masa_adiposa</td>
                                                }

                                                if (oEvNutricional.estado_dos == 1)
                                                {
                                                    <td style="border: 2px solid black; background-color: limegreen; color: white; ">@oEvNutricional.porcent_musculo</td>
                                                }
                                                else if (oEvNutricional.estado_dos == 2)
                                                {
                                                    <td style="border: 2px solid black; background-color: yellow; color: black;">@oEvNutricional.porcent_musculo</td>
                                                }
                                                else if (oEvNutricional.estado_dos == 3)
                                                {
                                                    <td style="border: 2px solid black; background-color: red; color: white;">@oEvNutricional.porcent_musculo</td>
                                                }

                                                if (oEvNutricional.estado_dos_dos == 1)
                                                {
                                                    <td style="border: 2px solid black; background-color: limegreen; color: white; ">@oEvNutricional.kilo_musculo</td>
                                                }
                                                else if (oEvNutricional.estado_dos_dos == 2)
