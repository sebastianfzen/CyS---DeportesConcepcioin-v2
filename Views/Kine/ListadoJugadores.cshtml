        <!--Funcion filtrar datos a las tablas-->
        <script>
            $(document).ready(function () {
                $("#buscarJugadoresKine").on("keyup", function () {
                    var value = $(this).val().toLowerCase();
                    $("#jugadoresUsersKine tr").filter(function () {
                        $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
                    });
                });
            });
        </script>

